{
  "name": "@qiwi/uniconfig-plugin-argv",
  "version": "3.0.1",
  "description": "Uniconfig argv reader plugin",
  "main": "target/es5/index.js",
  "types": "target/es5/index.d.ts",
  "scripts": {
    "clean": "rm -rf target",
    "jest": "NODE_ENV=test BAZ=baz jest -w 1 --detectOpenHandles --config jest.config.json",
    "build:es5": "mkdir -p target/es5 && tsc -p tsconfig.build.json --target ES5 --outDir target/es5 -m CommonJS",
    "build:es6": "mkdir -p target/es6 && tsc -p tsconfig.build.json --target ES6 --outDir target/es6",
    "build": "yarn build:es6 && yarn build:es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/uniconfig.git"
  },
  "keywords": [
    "universal config",
    "unified config"
  ],
  "author": "Qiwi <opensource@qiwi.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qiwi/uniconfig/issues"
  },
  "homepage": "https://github.com/qiwi/uniconfig#readme",
  "dependencies": {
    "@qiwi/uniconfig-core": "^3.0.0",
    "tslib": "^1.10.0",
    "yargs": "^13.2.4"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "target"
  ],
  "devDependencies": {
    "typescript": "3.5.2",
    "@types/node": "^12.0.10",
    "ts-jest": "^24.0.2",
    "@types/jest": "^24.0.15",
    "jest": "^24.8.0"
  }
}

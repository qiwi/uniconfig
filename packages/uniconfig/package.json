{
  "name": "@qiwi/uniconfig",
  "version": "3.1.0",
  "description": "All-in-one uniconfig pack",
  "main": "target/es5/index.js",
  "types": "target/es5/index.d.ts",
  "scripts": {
    "clean": "rm -rf target",
    "jest": "NODE_ENV=test jest -w 1 --detectOpenHandles --config jest.config.json",
    "build:es5": "mkdir -p target/es5 && tsc -p tsconfig.build.json --target ES5 --outDir target/es5 -m CommonJS",
    "build:es6": "mkdir -p target/es6 && tsc -p tsconfig.build.json --target ES6 --outDir target/es6",
    "build:bundle": "parcel build target/es6/index.js --out-dir target/bundle --experimental-scope-hoisting",
    "build": "yarn build:es6 && yarn build:es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/uniconfig.git"
  },
  "keywords": [
    "universal config",
    "unified config",
    "uniconfig",
    "uniconf"
  ],
  "author": "Qiwi <opensource@qiwi.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qiwi/uniconfig/issues"
  },
  "homepage": "https://github.com/qiwi/uniconfig#readme",
  "dependencies": {
    "@qiwi/uniconfig-core": "^3.1.0",
    "@qiwi/uniconfig-plugin-ajv": "^3.0.1",
    "@qiwi/uniconfig-plugin-api-file": "^3.0.1",
    "@qiwi/uniconfig-plugin-api-http": "^3.0.1",
    "@qiwi/uniconfig-plugin-argv": "^3.0.1",
    "@qiwi/uniconfig-plugin-datatree": "^3.0.1",
    "@qiwi/uniconfig-plugin-dot": "^3.0.1",
    "@qiwi/uniconfig-plugin-dotenv": "^3.0.1",
    "@qiwi/uniconfig-plugin-env": "^3.0.1",
    "@qiwi/uniconfig-plugin-global": "^3.0.1",
    "@qiwi/uniconfig-plugin-ip": "^3.0.1",
    "@qiwi/uniconfig-plugin-json": "^3.0.1",
    "@qiwi/uniconfig-plugin-path": "^3.0.1",
    "@qiwi/uniconfig-plugin-pkg": "^3.0.1",
    "@qiwi/uniconfig-plugin-root": "^3.0.1",
    "@qiwi/uniconfig-plugin-yaml": "^3.0.1"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "target"
  ],
  "devDependencies": {
    "typescript": "3.5.3",
    "@types/node": "^12.7.2",
    "@types/lodash": "^4.14.137",
    "ts-jest": "^24.0.2",
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0"
  }
}

{
  "name": "@qiwi/uniconfig",
  "version": "1.13.7",
  "description": "Yet another one config processor. Weird. Slow. Our own.",
  "main": "dist/es6/index.js",
  "scripts": {
    "jest": "BABEL_ENV=test NODE_ENV=test jest -w 1 --detectOpenHandles --config jest.config.json",
    "build_es6": "flow-remove-types src/ --out-dir dist/es6/",
    "build_es5": "BABEL_ENV=production babel src --out-dir dist/es5/",
    "build_bundle": "parcel build src/index.js --out-dir dist/bundle --experimental-scope-hoisting",
    "build": "rm -rf dist && npm run build_es5 && npm run build_es6 & npm run build_bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/uniconfig.git"
  },
  "keywords": [
    "universal config",
    "unified config"
  ],
  "author": "Qiwi <opensource@qiwi.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qiwi/uniconfig/issues"
  },
  "homepage": "https://github.com/qiwi/uniconfig#readme",
  "dependencies": {
    "async-lock": "^1.1.3",
    "js-yaml": "^3.12.0",
    "lodash-es": "^4.17.10",
    "minimist": "^1.2.0",
    "semver": "^5.5.0"
  },
  "devDependencies": {},
  "files": [
    "README.md",
    "CHANGELOG.md",
    "dist/"
  ]
}

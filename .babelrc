{
  "comments": false,
  "ignore": [
    "interface.js"
  ],
  "env": {
    "production": {
      "presets": [
        "flow",
        ["env", {"modules": false}]
      ],
      "plugins": [
        "transform-object-rest-spread",
        "transform-class-properties"
      ]
    },
    "test": {
      "presets": [
        "flow",
        "env"
      ],
      "plugins": [
        "transform-es2015-modules-commonjs",
        "transform-class-properties"
      ]
    }
  }
}
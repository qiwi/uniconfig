{
  "name": "@qiwi/uniconfig-plugin-yaml",
  "version": "3.3.1",
  "description": "Uniconfig yaml parser plugin",
  "main": "target/es5/index.js",
  "types": "target/es5/index.d.ts",
  "scripts": {
    "clean": "rm -rf target",
    "jest": "NODE_ENV=test BAZ=baz jest -w 1 --config jest.config.json",
    "test": "yarn run jest",
    "build:es5": "mkdir -p target/es5 && tsc -p tsconfig.build.json --target ES5 --outDir target/es5 -m CommonJS",
    "build:es6": "mkdir -p target/es6 && tsc -p tsconfig.build.json --target ES6 --outDir target/es6",
    "build": "yarn build:es6 && yarn build:es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/uniconfig.git"
  },
  "keywords": [
    "universal config",
    "unified config"
  ],
  "author": "Qiwi <opensource@qiwi.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qiwi/uniconfig/issues"
  },
  "homepage": "https://github.com/qiwi/uniconfig#readme",
  "dependencies": {
    "@qiwi/uniconfig-core": "^3.3.0",
    "js-yaml": "^3.13.1",
    "tslib": "^2.0.0"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "target"
  ],
  "devDependencies": {
    "typescript": "3.8.3",
    "@types/node": "^13.13.4",
    "ts-jest": "^25.4.0",
    "@types/jest": "^25.2.1",
    "jest": "^25.5.4",
    "@types/js-yaml": "^3.12.3",
    "@qiwi/uniconfig-plugin-datatree": "^3.3.0",
    "@qiwi/uniconfig-plugin-api-file": "^3.3.0"
  }
}

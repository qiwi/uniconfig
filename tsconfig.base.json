{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es5",
    "lib": [
      "esnext"
    ],
    "strict": true,
    "sourceMap": true,
    "declaration": true,
    "noEmitHelpers": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true
  }
}

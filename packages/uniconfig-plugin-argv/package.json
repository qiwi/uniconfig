{
  "name": "@qiwi/uniconfig-plugin-argv",
  "version": "2.21.1",
  "description": "Uniconfig argv reader plugin",
  "main": "dist/es6/index.js",
  "scripts": {
    "build_es6": "flow-remove-types src/ --out-dir dist/es6/",
    "build_es5": "BABEL_ENV=production babel src --out-dir dist/es5/",
    "build": "rm -rf dist && npm run build_es6 && npm run build_es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/uniconfig.git"
  },
  "keywords": [
    "universal config",
    "unified config"
  ],
  "author": "Qiwi <opensource@qiwi.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qiwi/uniconfig/issues"
  },
  "homepage": "https://github.com/qiwi/uniconfig#readme",
  "dependencies": {
    "@qiwi/uniconfig-core": "^2.19.0",
    "yargs": "^12.0.5"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "dist/"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2"
  }
}

{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es5",
    "lib": [
      "esnext",
      "dom"
    ],
    "strict": true,
    "sourceMap": true,
    "declaration": true,
    "noEmitHelpers": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,

    "incremental": true,
    "composite": true,
    "tsBuildInfoFile": "./buildcache/.tsbuildinfo"
  }
}
